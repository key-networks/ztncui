extends controller_layout

block content
  if error
     b #{error}
  else
    .row
      .col-sm-10
        h2
          a(href='/controller/network/' + network.nwid) #{network.name}
          |  (#{network.nwid}):
        h3= title

      .col-sm-2
        h2.right
          a.btn.btn-default(href=nav.whence role='button') Back
    block net_content
